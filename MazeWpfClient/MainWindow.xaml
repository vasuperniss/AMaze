<Window x:Class="MazeWpfClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MazeWpfClient"
        mc:Ignorable="d"
        Title="Maze Game - Main Menu" Height="350" Width="500" ResizeMode="NoResize" WindowStyle="ToolWindow" Icon="icon-maze.ico">
    <Window.Effect>
        <DropShadowEffect Color="#FF4D0D4A"/>
    </Window.Effect>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="27*"/>
            <RowDefinition Height="82*"/>
        </Grid.RowDefinitions>
    </Grid>
</Window>
