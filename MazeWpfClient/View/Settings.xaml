<Window x:Class="MazeWpfClient.View.Settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MazeWpfClient.View"
        mc:Ignorable="d"
        Title="Settings" Height="300" Width="300">
    <Grid>
        <StackPanel Height="249" Margin="10,10,10,0" VerticalAlignment="Top">
            <Label x:Name="label" Content="IP Address" FontFamily="Tahoma" Margin="5,0" FontSize="16"/>
            <TextBox x:Name="textBox" Height="23" TextWrapping="Wrap" Text="TextBox" Margin="10,0" FontFamily="Tahoma">
            </TextBox>
            <Label x:Name="label1" Content="Port&#xD;&#xA;" Height="28" FontFamily="Tahoma" Margin="5,0" FontSize="16"/>
            <TextBox x:Name="textBox1" Height="23" TextWrapping="Wrap" Text="TextBox" Margin="10,0" FontFamily="Tahoma"/>
        </StackPanel>

    </Grid>
</Window>
